{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "baseUrl": "./src",
        "typeRoots": [
            "./src"
        ],
        "types": [],
        "forceConsistentCasingInFileNames": true
    }
}